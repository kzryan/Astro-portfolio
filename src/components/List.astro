---
export interface Props {
  items: { title: string; description: string }[];
}
const { items } = Astro.props as Props;
---

<ul>
  {items.map(item => (
    <li>
      <strong>{item.title}:</strong> {item.description}
    </li>
  ))}
</ul>

<style>
    ul {
        list-style-type: none;
        padding: 0;
        }

    li {
    margin: 10px 0;
    font-size: 1.2em;
    }

    li:hover {
    color: #007BFF;
    }
</style>